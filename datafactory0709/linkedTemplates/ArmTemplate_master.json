{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory0709"
		},
		"LN_adls_Gen2_devaccount_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datafactory0709storage.dfs.core.windows.net/"
		},
		"LN_azuresql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "devdk2024sep.database.windows.net"
		},
		"LN_azuresql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dev-db"
		},
		"LN_azuresql_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "bf3672d9-59d2-423a-81e5-b9bea67642e0"
		},
		"LN_azuresql_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "353f4a9d-db4b-45f6-afc6-03ae09877325"
		},
		"LN_dev_akv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://databricks-secret-255.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory0709_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LN_adls_Gen2_devaccount_properties_typeProperties_url": {
						"value": "[parameters('LN_adls_Gen2_devaccount_properties_typeProperties_url')]"
					},
					"LN_dev_akv_properties_typeProperties_baseUrl": {
						"value": "[parameters('LN_dev_akv_properties_typeProperties_baseUrl')]"
					},
					"LN_azuresql_properties_typeProperties_server": {
						"value": "[parameters('LN_azuresql_properties_typeProperties_server')]"
					},
					"LN_azuresql_properties_typeProperties_database": {
						"value": "[parameters('LN_azuresql_properties_typeProperties_database')]"
					},
					"LN_azuresql_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('LN_azuresql_properties_typeProperties_servicePrincipalId')]"
					},
					"LN_azuresql_properties_typeProperties_tenant": {
						"value": "[parameters('LN_azuresql_properties_typeProperties_tenant')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}