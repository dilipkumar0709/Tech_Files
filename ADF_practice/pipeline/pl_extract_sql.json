{
	"name": "pl_extract_sql",
	"properties": {
		"activities": [
			{
				"name": "Pl_extract_from_sql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"outputs": [
					{
						"referenceName": "ds_generic_adls_csv",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Framework"
		},
		"annotations": []
	}
}